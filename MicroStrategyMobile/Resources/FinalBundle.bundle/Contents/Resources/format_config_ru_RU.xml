	<locale language="ru" country="RU">
        <list-separator>;</list-separator>
		<data-type name="integer">
			<out>###0</out>
			<in>#&#xa0;##0</in>
                        <!-- display node for this data type currently not in use -->
			<display>###0</display>
		</data-type>
		<data-type name="number">
			<out>###0,##########</out>
			<in>#&#xa0;##0,###</in>
                        <!-- display node for this data type currently not in use -->
			<display>###0,##########</display>
		</data-type>
		<data-type name="date">
			<out>dd.MM.yyyy</out>
			<in>dd.MM.yyyy</in>
			<in>dd.MM.yy</in>
			<in>d.M.yyyy</in>
			<in>dd/MM/yy</in>
			<in>yyyy-MM-dd</in>
		<display>dd.MM.yyyy</display>
		</data-type>
		<data-type name="time">
			<out>H:mm:ss</out>
			<in>H:mm:ss</in>
			<in>HH:mm:ss</in>
                        <display>H:mm:ss</display>
		</data-type>
     </locale>
