	<locale language="es" country="ES">
		<list-separator>,</list-separator>
		<data-type name="integer">
			<out>###0</out>
			<in>#.##0</in>
                        <!-- display node for this data type currently not in use -->
			<display>###0</display>
		</data-type>
		<data-type name="number">
			<out>###0,##########</out>
			<in>#.##0,###</in>
                        <!-- display node for this data type currently not in use -->
			<display>###0,##########</display>
		</data-type>
		<data-type name="date">
			<out>d/MM/yyyy</out>
			<in>d/M/yyyy</in>
			<in>d/M/yy</in>
			<in>d-M-yy</in>
			<in>d.M.yy</in>
			<in>yyyy-M-d</in>
                        <display>d/MM/yy</display>
		</data-type>
		<data-type name="time">
			<out>H:mm:ss</out>
			<in>H:mm</in>
			<in>H:mm:ss</in>
			<in>H'H'mm''</in>
			<in>H:mm:ss z</in>
                        <display>H:mm:ss</display>
		</data-type>
	</locale>
