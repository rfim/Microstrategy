	<locale language="en" country="GB">
		<list-separator>,</list-separator>
		<data-type name="integer">
			<out>###0</out>
			<in>#,##0</in>
                        <!-- display node for this data type currently not in use -->
			<display>###0</display>
		</data-type>
		<data-type name="number">
			<out>###0.##########</out>
			<in>#,##0.###</in>
                        <!-- display node for this data type currently not in use -->
			<display>###0.##########</display>
		</data-type>
		<data-type name="date">
			<out>dd/MM/yyyy</out>
			<in>dd/MM/yy</in>
			<in>dd/MM/yyyy</in>
			<in>dd-MMM-yy</in>
			<in>dd MMMM yy</in>
                        <display>dd/MM/yy</display>
		</data-type>
		<data-type name="time">
			<out>HH:mm:ss</out>
			<in>HH:mm</in>
			<in>HH:mm:ss</in>
			<in>HH:mm:ss z</in>
                        <display>HH:mm:ss</display>
		</data-type>
	</locale>
