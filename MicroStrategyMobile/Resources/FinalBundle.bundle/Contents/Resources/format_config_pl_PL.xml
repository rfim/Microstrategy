	<locale language="pl" country="PL">
        <list-separator>;</list-separator>
		<data-type name="integer">
			<out>###0</out>
			<in>#&#xa0;##0</in>
                        <!-- display node for this data type currently not in use -->
			<display>###0</display>
		</data-type>
		<data-type name="number">
			<out>###0,##########</out>
			<in>#&#xa0;##0,###</in>
                        <!-- display node for this data type currently not in use -->
			<display>###0,##########</display>
		</data-type>
		<data-type name="date">
			<out>yyyy-MM-dd</out>
			<in>yyyy-MM-dd</in>
			<in>yy-MM-dd</in>
		<display>yyyy-MM-dd</display>
		</data-type>
		<data-type name="time">
			<out>HH:mm:ss</out>
			<in>HH:mm:ss</in>
			<in>H:mm:ss</in>
                        <display>HH:mm:ss</display>
		</data-type>
	</locale>
