    <locale language="zh" country="TW">
        <list-separator>,</list-separator>
        <data-type name="integer">
            <out>###0</out>
            <in>#,##0</in>
            <!-- display node for this data type currently not in use -->
           <display>###0</display>
        </data-type>
        <data-type name="number">
            <out>###0.##########</out>
            <in>#,##0.###</in>
            <!-- display node for this data type currently not in use -->
            <display>###0.##########</display>
        </data-type>
        <data-type name="date">
            <out>yyyy/MM/dd</out>
            <in>yyyy/M/d</in>
            <in>yyyy-MM-dd</in>
            <in>M/d</in>
            <in>M/d/yy</in>
            <display>yyyy/M/d</display>
        </data-type>
        <data-type name="time">
            <out>H:mm:ss</out>
            <in>H:m:s</in>
            <in>H:m</in>
            <in>h:m a</in>
            <in>H:m:s a</in>
            <display>hh:mm:ss</display>
        </data-type>
</locale>
