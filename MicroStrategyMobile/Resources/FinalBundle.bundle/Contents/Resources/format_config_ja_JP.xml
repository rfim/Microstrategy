	<locale language="ja" country="JP">
		<list-separator>,</list-separator>
		<data-type name="integer">
			<out>###0</out>
			<in>#,##0</in>
                        <!-- display node for this data type currently not in use -->
			<display>###0</display>
		</data-type>
		<data-type name="number">
			<out>###0.##########</out>
			<in>#,##0.###</in>
                        <!-- display node for this data type currently not in use -->
			<display>###0.##########</display>
		</data-type>
		<data-type name="date">
			<out>yyyy/MM/dd</out>
			<in>yy/MM/dd</in>
			<in>yyyy/MM/dd</in>
			<in>yy/MM/dd (E)</in>
			<in>yyyy/MM/dd (E)</in>
			<in>yyyy-MM-dd</in>
                        <display>yy/MM/dd</display>
		</data-type>
		<data-type name="time">
			<out>H:mm:ss</out>
			<in>H:mm</in>
			<in>H:mm:ss</in>
			<in>H:mm:ss:z</in>
                        <display>H:mm:ss</display>
		</data-type>
	</locale>
